{
  "expo": {
    "name": "ConstellationMS Job List",
    "slug": "cms-jobs",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/mylogo.png",
    "scheme": "cmsjobs",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/mylogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#66a4de"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/mylogo.png",
        "backgroundColor": "#66a4de"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.constellationms.cmsjobs"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/mylogo.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "da9fac10-aa40-46cd-b012-f455a0504432"
      }
    }
  }
}
